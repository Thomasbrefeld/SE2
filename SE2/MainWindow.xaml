<Window x:Class="SE2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SE2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="400">
    <Grid>
        <TabControl>
            <TabItem Header="Main">
                <Label Content="Main page content" />
            </TabItem>
            <TabItem Header="To-Do List">
                <Label Content="To-Do List content" />
            </TabItem>
            <TabItem Header="Reminders">
                <Label Content="Reminders content" />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
